{
  "expo": {
    "name": "SnapSort",
    "slug": "snapsort",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.anonymous.snapsort"
    },
    "web": {
      "output": "server"
    },
    "sdkVersion": "53.0.0",
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-audio",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5724954c-d3aa-4a15-b10d-cc264171827d"
      }
    }
  }
}
