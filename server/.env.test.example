# Test Environment Variables
# Copy this file to .env.test and update with your test values

NODE_ENV=test

# Supabase Test Configuration
SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_ANON_KEY=your-test-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-test-service-role-key

# OpenAI Test Configuration
OPENAI_API_KEY=sk-test-your-openai-key

# Server Configuration
PORT=3001
HOST=localhost

# Redis Test Configuration (optional)
REDIS_URL=redis://localhost:6379/1

# Rate Limiting (relaxed for testing)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# AWS Configuration (for testing)
AWS_ACCESS_KEY_ID=test-access-key
AWS_SECRET_ACCESS_KEY=test-secret-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=test-bucket

# Monitoring (disabled for tests)
ENABLE_METRICS=false
ENABLE_LOGGING=false
